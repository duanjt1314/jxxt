@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>模块管理</title>
    <link href="/Content/CSS/icon.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Script/ExtJs/resources/css/ext-all.css" rel="stylesheet"
        type="text/css" />
    <script src="/Content/Script/ExtJs/ext-all.js" type="text/javascript"></script>
    <script src="/Content/Script/ExtJs/locale/ext-lang-zh_CN.js" type="text/javascript"></script>

    <script src="/Content/Function/LifeMan/Module/ModuleTree.js" type="text/javascript"></script>
    <script src="/Content/Function/LifeMan/Module/ModuleGrid.js" type="text/javascript"></script>
    <script type="text/javascript">
        //全局变量
        var parentId = '-1';
        var iconId = "";//选择图标需要修改的文本框的Id
        
        /*界面初始化*/
        Ext.onReady(function () {
            Ext.create("Ext.container.Viewport", {
                layout: "border",
                items: [moduleTree, moduleGrid],
                listeners: {
                    "afterrender": function () {
                        parent.myMask.hide();
                    }
                }
            });
            moduleTree.store.load();
        });
    </script>
    <style type="text/css">
        .thumb-wrap img
        {
            margin: 4px;            
        }
        .thumb-wrap
        {
            width: 24px;
            height: 24px;
            display: block;
            float: left;
        }
        .thumb-wrap:hover
        {
            background-color: Gray;
            cursor: pointer;
        }
    </style>
</head>
<body>
</body>
</html>